# This file defines all module routes (Higher priority routes first)
#
# import these routes in the main app as :
# *     /                       module:play-rabbitmq
#
# ~~~~

GET     /?                      Module.index

GET  /rabbitmq  									RabbitMQStats.index
GET  /rabbitmq/queue/{queueName}/stats  			RabbitMQStats.queueStats
GET  /rabbitmq/queue/{queueName}/producer/success  	RabbitMQStats.producerSuccess
GET  /rabbitmq/queue/{queueName}/producer/failed  	RabbitMQStats.producerFailed
GET  /rabbitmq/queue/{queueName}/consumer/success  	RabbitMQStats.consumerSuccess
GET  /rabbitmq/queue/{queueName}/consumer/failed  	RabbitMQStats.consumerFailed